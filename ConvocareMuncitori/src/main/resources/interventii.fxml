<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<GridPane fx:controller="org.loose.fis.sre.controllers.InterventiiController"
          xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" minHeight="1000" minWidth="3900"
          vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal"
          prefHeight="400.0" prefWidth="600.0">
    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
    </padding>
    <Text id="text" text="Lista interventii" GridPane.columnIndex="0" GridPane.columnSpan="2"
          GridPane.rowIndex="0"/>

    <ListView fx:id="listaInterventii"/>

    <Label text="Inapoi la dashboard" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <HBox alignment="bottom_right" spacing="10" GridPane.columnIndex="1" GridPane.rowIndex="3">
        <Button onAction="#gotoDashboard" text="Inapoi"/>
    </HBox>
</GridPane>
